@main(cmd_args) {
	declare $var "Variable!";
	/ This will declare a nameless variable
	  that will return 0i32. /
	print "Hello";
	print "Hello \ world!";
	print "Hi!" 33;
	print "Greetings!";
	print $var;
	print "Other";
	print "Hello  world!" "Hello world!" [33 33 33];
	print "";
	declare $hello_world "Test.";
	print $hello_world;
	second
}

@second(arg1 arg2) {
	print "Greetings!";
}

@arguments(arg1 arg2) {
	print $arg1 $arg2;
}